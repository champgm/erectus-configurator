  {
    "extends": "tslint-config-airbnb",
    "linterOptions": {
      "exclude": [
        "**/out/**",
        "**/node_modules/**"
      ]
    },
    "rules": {
      "max-line-length": [
        true,
        200
      ],
      "import-name": false,
      "strictNullChecks": false,
      "no-boolean-literal-compare": true,
      "whitespace": false,
      "quotemark": {
        "severity": "warn",
        "options": [
          "single"
        ]
      },
      "prefer-template": false
    }
  }
