{
  "name": "erectus-configurator",
  "displayName": "erectus-configurator",
  "description": "erectus-configurator",
  "publisher": "champgm",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yaml",
    "onCommand:extension.annotateIds"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.annotateIds",
        "title": "Erectus Configurator: Annotate known Base IDs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "clean": "rimraf out/ && rimraf coverage/",
    "compile": "npm run clean && tsc -p ./",
    "eslint": "eslint src --ext ts",
    "lint": "tslint src/**/*.ts --fix --project tsconfig.json",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/ini": "^1.3.30",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "ini": "^1.3.6",
    "lodash": "^4.17.15"
  }
}
